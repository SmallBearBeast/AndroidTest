<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatefulView">
        <!-- 背景相关公共属性 -->
        <attr name="sf_solid" format="reference|color" />
        <attr name="sf_corner_radius" format="dimension" />
        <attr name="sf_left_top_corner_radius" format="dimension" />
        <attr name="sf_left_bottom_corner_radius" format="dimension" />
        <attr name="sf_right_top_corner_radius" format="dimension" />
        <attr name="sf_right_bottom_corner_radius" format="dimension" />
        <attr name="sf_stroke_width" format="dimension" />
        <attr name="sf_stroke_color" format="color" />
        <attr name="sf_stroke_dash_width" format="dimension" />
        <attr name="sf_stroke_dash_gap" format="dimension" />
        <attr name="sf_gra_start_color" format="color" />
        <attr name="sf_gra_center_color" format="color" />
        <attr name="sf_gra_end_color" format="color" />
        <attr name="sf_gra_orientation" format="enum">
            <enum name="top_bottom" value="0" />
            <enum name="tr_bl" value="1" />
            <enum name="right_left" value="2" />
            <enum name="br_tl" value="3" />
            <enum name="bottom_top" value="4" />
            <enum name="bl_tr" value="5" />
            <enum name="left_right" value="6" />
            <enum name="tl_br" value="7" />
        </attr>

        <attr name="sf_pressed_solid" format="reference|color" />
