        <attr name="sf_pressed_left_top_corner_radius" format="dimension" />
        <attr name="sf_pressed_left_bottom_corner_radius" format="dimension" />
        <attr name="sf_pressed_right_top_corner_radius" format="dimension" />
        <attr name="sf_pressed_right_bottom_corner_radius" format="dimension" />
        <attr name="sf_pressed_stroke_width" format="dimension" />
        <attr name="sf_pressed_stroke_color" format="color" />
        <attr name="sf_pressed_stroke_dash_width" format="dimension" />
        <attr name="sf_pressed_stroke_dash_gap" format="dimension" />
        <attr name="sf_pressed_gra_start_color" format="color" />
        <attr name="sf_pressed_gra_center_color" format="color" />
        <attr name="sf_pressed_gra_end_color" format="color" />
        <attr name="sf_pressed_gra_orientation" format="enum">
            <enum name="top_bottom" value="0" />
            <enum name="tr_bl" value="1" />
            <enum name="right_left" value="2" />
            <enum name="br_tl" value="3" />
            <enum name="bottom_top" value="4" />
            <enum name="bl_tr" value="5" />
            <enum name="left_right" value="6" />
            <enum name="tl_br" value="7" />
        </attr>

        <attr name="sf_selected_solid" format="reference|color" />
        <attr name="sf_selected_corner_radius" format="dimension" />
        <attr name="sf_selected_left_top_corner_radius" format="dimension" />
        <attr name="sf_selected_left_bottom_corner_radius" format="dimension" />
        <attr name="sf_selected_right_top_corner_radius" format="dimension" />
        <attr name="sf_selected_right_bottom_corner_radius" format="dimension" />
        <attr name="sf_selected_stroke_width" format="dimension" />
        <attr name="sf_selected_stroke_color" format="color" />
        <attr name="sf_selected_stroke_dash_width" format="dimension" />
        <attr name="sf_selected_stroke_dash_gap" format="dimension" />
        <attr name="sf_selected_gra_start_color" format="color" />
